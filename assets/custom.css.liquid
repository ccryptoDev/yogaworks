.quick-add__submit.button:not([disabled]):hover:after {
    /* --border-offset:  */
}
@media screen and (max-width: 768px) {
    ._mobile.slide-image__wrapper {
        margin-bottom: 24px;
    }
    .slide-image__wrapper .slide-image__caption {
        max-width: 100%;
    }
    .menu-drawer__account.link{
       display: none;
    }
}

/* footer */
footer.footer {
    padding: 16px !important;
    letter-spacing: normal;
    font-size: 13px;
}
.footer__content-top {
    border: none;
    display: flex;
    justify-content: space-between;
    gap: 24px;
    font-size: 13px;
    padding: 0;
}
.footer-block h2 {
    color: #000;
    font-size: 13px;
    font-weight: 700;
    text-transform: uppercase;
    margin-top: 0;
    margin-bottom: 24px;
    letter-spacing: normal;
}
.footer-block__newsletter h2 {
    margin-bottom: 15px;
}
.footer-block--menu ul > li {
    font-weight: 700;
    margin-bottom: 8px;
}
.footer-block ul > li a {
    color: #000;
    text-decoration: none;
}
.footer-block ul > li a:hover {
    text-decoration: underline;
}
.footer-absolute {
    max-width: 1188px;
    margin: auto;
    display: flex;
    gap: 32px;
    justify-content: center;
    padding: 45px 0 80px;
}
.subscribe-wrapper {
    width: 368px;
}
.newsletter-form__field-wrapper .field::before,
.newsletter-form__field-wrapper .field::after {
    content: none;
}
.newsletter-form__field-wrapper .field__input:focus,
.newsletter-form__field-wrapper .field__input:focus-visible {
    outline: none;
    border: 1px solid #7e7d7d;
    box-shadow: none;
}
.newsletter-form__field-wrapper .field label {
    font-size: 13px;
    letter-spacing: normal;
    line-height: 1.7;
    text-transform: lowercase;
}
.newsletter-form__field-wrapper .field__label {
    left: 2rem;
    top: 1rem;
}
.newsletter-form__field-wrapper .field__input:focus ~ .field__label,
.newsletter-form__field-wrapper .field__input:not(:placeholder-shown) ~ .field__label {
    left: 2rem;
    top: 1rem;
    font-size: 13px;
    letter-spacing: normal;
}
.hide-label {
    display: none;
}
.newsletter-form__field-wrapper .field__button > svg {
    width: 7px;
    height: 13px;
}
.list-social {
    display: flex;
    gap: 24px;
}
.footer-absolute > a {
    color: #000;
    font-size: 11px;
    text-decoration: underline;
}
.footer-absolute > a:hover {
    text-decoration: none;
}
.footer-logo.mobile {
    display: none;
}
.social.mobile {
    display: none;
}
@media (max-width: 768px) {
    .footer__content-top {
        display: grid !important;
        grid-template-columns: repeat(3, 1fr);
        gap: 46px !important;
    }
    .footer-logo.desktop {
        display: none;
    }
    .footer-logo.mobile {
        grid-row: 2 / span 3;
        grid-column: 3;
        display: flex;
        justify-content: flex-end;
    }
    .footer__content-top > .footer-block--menu:nth-child(1) {
        grid-row: 2;
        grid-column: 1 / span 2;
        width: 100%;
    }
    .footer__content-top > .footer-block--menu:nth-child(2) {
        grid-row: 3;
        grid-column: 1 / span 2;
        width: 100%;
    }
    .subscribe-social {
        grid-row: 1;
        grid-column: 1 / span 3;
        width: 100%;
    }
    .social.desktop {
        display: none;
    }
    .social.mobile {
        grid-row: 4;
        grid-column: 1 / span 2;
        display: flex;
        gap: 24px;
    }
    .subscribe-wrapper {
        width: 100%;
    }
}
/* footer */


.title.inline-richtext{
    font-weight: 700;
}

.full-blank-row{
    background-color: #000;
    padding-top: 5px;
    padding-bottom: 5px;
}

.row-full-width-inner{
    max-width: var(--page-width);
    margin: 0px auto;
    padding-inline: 16px;
}

.theme-arrow--white {
    border-color: #fff;
}



.theme-arrow {
    position: relative;
    max-width: 100%;
    min-width: 24px;
    flex-grow: 1;
    height: 0;
    border-top: 1px solid #000;
    display: block !important;
}

.theme-arrow--white {
    border-color: #fff;
}

.theme-arrow:before {
    content: '';
    width: 6px;
    position: absolute;
    height: 0;
    right: -1px;
    bottom: 2px;
    border-top: 1px solid #000;
    transform: rotate(45deg);
}

.theme-arrow:after {
    content: '';
    width: 6px;
    position: absolute;
    height: 0;
    right: -1px;
    top: 1px;
    border-top: 1px solid #000;
    transform: rotate(-45deg);
}

.theme-arrow--white:after, 
.theme-arrow--white:before {
    border-color: #fff;
}
.theme-arrow--no-arrowhead:after, 
.theme-arrow--no-arrowhead:before {
    display: none;
}

@media (min-width: 1440px){
    .slick-slider .slick-prev {
        left: -36px;
    }
    .slick-slider .slick-next {
        right: -36px;
    }
}

@media (min-width: 768px) {
    .footer__content-top {
        box-sizing: border-box;
        max-width: 1188px;
        padding-left: 0;
        padding-right: 0;
        width: auto;
        margin: 0 auto;
    }
}