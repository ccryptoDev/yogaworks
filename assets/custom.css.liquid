/* Public Sans font */
/* Normal */
@font-face {
    font-family: 'Public Sans';
    src: url('https://cdn.shopify.com/s/files/1/0888/3696/4634/files/PublicSans-Regular.woff2?v=1732228283') format('woff2'),
         url('https://cdn.shopify.com/s/files/1/0888/3696/4634/files/PublicSans-Regular.woff?v=1732228283') format('woff');
    font-weight: 400;
    font-style: normal;
}
@font-face {
    font-family: 'Public Sans Medium';
    src: url('https://cdn.shopify.com/s/files/1/0888/3696/4634/files/PublicSans-Medium.woff2?v=1732228283') format('woff2'),
         url('https://cdn.shopify.com/s/files/1/0888/3696/4634/files/PublicSans-Medium.woff?v=1732228283') format('woff');
    font-weight: 500;
    font-style: normal;
}
@font-face {
    font-family: 'Public Sans SemiBold';
    src: url('https://cdn.shopify.com/s/files/1/0888/3696/4634/files/PublicSans-SemiBold.woff2?v=1732228283') format('woff2'),
         url('https://cdn.shopify.com/s/files/1/0888/3696/4634/files/PublicSans-SemiBold.woff?v=1732228283') format('woff');
    font-weight: 600;
    font-style: normal;
}
@font-face {
    font-family: 'Public Sans';
    src: url('https://cdn.shopify.com/s/files/1/0888/3696/4634/files/PublicSans-Bold.woff2?v=1732228283') format('woff2'),
         url('https://cdn.shopify.com/s/files/1/0888/3696/4634/files/PublicSans-Bold.woff?v=1732228283') format('woff');
    font-weight: 700;
    font-style: normal;
}

/* Italic */
@font-face {
    font-family: 'Public Sans Italic';
    src: url('https://cdn.shopify.com/s/files/1/0888/3696/4634/files/PublicSans-Italic.woff2?v=1732228283') format('woff2'),
         url('https://cdn.shopify.com/s/files/1/0888/3696/4634/files/PublicSans-Italic.woff?v=1732228283') format('woff');
    font-weight: 400;
    font-style: italic;
}
@font-face {
    font-family: 'Public Sans MediumItalic';
    src: url('https://cdn.shopify.com/s/files/1/0888/3696/4634/files/PublicSans-MediumItalic.woff2?v=1732228283') format('woff2'),
         url('https://cdn.shopify.com/s/files/1/0888/3696/4634/files/PublicSans-MediumItalic.woff?v=1732228283') format('woff');
    font-weight: 500;
    font-style: italic;
}
@font-face {
    font-family: 'Public Sans SemiBold';
    src: url('https://cdn.shopify.com/s/files/1/0888/3696/4634/files/PublicSans-SemiBoldItalic.woff2?v=1732228283') format('woff2'),
         url('https://cdn.shopify.com/s/files/1/0888/3696/4634/files/PublicSans-SemiBoldItalic.woff?v=1732228283') format('woff');
    font-weight: 600;
    font-style: italic;
}
@font-face {
    font-family: 'Public Sans BoldItalic';
    src: url('https://cdn.shopify.com/s/files/1/0888/3696/4634/files/PublicSans-BoldItalic.woff2?v=1732228283') format('woff2'),
         url('https://cdn.shopify.com/s/files/1/0888/3696/4634/files/PublicSans-BoldItalic.woff?v=1732228283') format('woff');
    font-weight: 700;
    font-style: italic;
}
html, body {
    overflow-x: hidden;
}
html *, body * {
    box-sizing: border-box;
}
html {
    font-size: 62.5%;
    -webkit-text-size-adjust: 100%;
    -ms-text-size-adjust: 100%;
    font-size-adjust: 100%;
}
body {
    color: #000;
    background-color: #fff;
    font-family: 'Public Sans', Helvetica, sans-serif;
    font-style: normal;
    font-weight: 400;
    line-height: 1.33333;
    font-size: 15px;
    scroll-behavior: smooth;
    margin: 0;
    padding: 0;

}

/* GLOBAL STYLES */
/** this overwrites global page width in settings which accepts only absolute values */
.page-width {
    max-width: 1188px;
    width: 100%;
    margin: 0 auto!
}
.page-inner {
    width: 100%;
    padding-inline: 16px;
}

/* Anouncement bar */
.header-wrapper--border-bottom {
    border-bottom: none;
}
.announcement-bar__message {
    &.h5 {
        color: inherit;
        text-transform: none;
    }
}
.announcement-bar__link {
    min-height: 26px;
    padding: 5px;
}
.announcement-bar__message {
    padding: 0;
    min-height: 0;
    letter-spacing: normal;
    line-height: 14px;
    font-size: 15px;
}
.announcement-bar__link {
    .icon-arrow {
        display: none;
    }
    &:hover {
        text-decoration: none;
    }
}
/* Anouncement bar */

/* HEADER STYLES */
.header {
    display: block;
}
.header-logo {
    padding: 14px;
}
.logo-wrapper {
    display: flex;
    justify-content: center;
}
.menu {
    position: relative;
}
.menu-content {
    width: 100%;
    max-width: 1188px;
    display: flex;
    gap: 16px;
    justify-content: space-between;
    align-items: end;
    margin: auto;
    padding: 12px 16px;
}
.menu-right {
    /* position: relative; */
    display: flex;
    z-index: 400;
    justify-content: flex-end;
    align-items: flex-end;
    gap: 16px;
    min-width: 250px;
}
.navigation-content {
    max-width: 1188px;
    margin: auto;
    padding-inline: 16px !important;
}
.navigation .topmenu-bottom a {
    text-transform: uppercase;
    color: #000;
    text-decoration: none;
}
.topmenu-bottom a:hover {
    text-decoration: underline;
}
@media (min-width: 769px) {
    .menu-right {
        min-width: 250px;
    } 
    .header__inline-menu,
    .navigation {
        display: block;
        max-width: unset;
        width: calc(100vw - 16px);
        position: absolute;
        top: 8px;
        left: 0;
        font-size: 12px;
        line-height: 16px;
        max-height: 40px;
        overflow: hidden;
        transition: max-height .1s linear;
        background: #fff;
        z-index: 300;
        padding-left: 0;
        padding-right: 0;
        font-weight: 700;
    }
    .navigation .topmenu {
        width: 100%;
        padding: 12px 0;
        display: flex;
    }
    .navigation .topmenu .level0.level-top {
        position: relative;
        margin-right: 48px;
        display: flex;
        text-transform: uppercase;
        flex-direction: column;
        transition: min-width linear .1s, margin-right linear .1s;
    }
    .navigation .level0 > .level-top {
        color: #000;
        line-height: 53px;
        padding: 0 12px;
        text-decoration: none;
        box-sizing: border-box;
        position: relative;
        display: inline-block;
    }
    .navigation .topmenu .level0 a {
        border: none !important;
        line-height: 16px !important;
        padding: 0 !important;
    }
    .navigation .topmenu .submenu {
        display: none;
        position: static;
        margin-top: 20px !important;
        border: none;
        box-shadow: none;
        min-width: unset;
        background: 0 0;
        font-weight: 700;
        margin: 0;
        z-index: 1;
    }
    .navigation .level0 .submenu:before {
        content: '';
        display: block;
        position: absolute;
        width: 100%;
        height: 4px;
        left: 0;
        top: -4px;
        z-index: 1;
    }
    .navigation .level0 .submenu li {
        margin: 0;
        position: relative;
    }
    .navigation .topmenu .submenu a {
        margin-bottom: 6px;
        text-transform: none;
        display: block;
        color: #000;
        text-decoration: none;
    }
    .navigation .topmenu-bottom {
        display: none;
        margin: 30px 0;
    }
    .navigation .topmenu-bottom-right {
        display: flex;
        gap: 5px;
        align-items: center;
    }
    .navigation .topmenu-bottom-right .theme-arrow {
        width: 18px
    }

    .navigation.open {
        width: 100%;
    }
    .navigation.open:has(.submenu.open) {
        max-height: 300px;
        border-bottom: 1px solid #000;
    }
    .navigation.open .submenu.open {
        display: block;
    }
    .navigation.open .level0.level-top {
        margin-right: 24px;
        min-width: 180px !important;
        max-width: 180px !important;
    }
    .navigation.open:has(.submenu.open) .level0.level-top > a {
        text-decoration: underline;
    }
    .navigation .topmenu .submenu a:hover {
        background: 0 0;
        text-decoration: underline
    }
    .navigation.open:has(.submenu.open) .topmenu-bottom {
        display: flex;
        justify-content: space-between
    }
    .header-right.header__icons {
        display: none !important;
    }
    .header__icon {
        height: auto;
        width: 3.4rem;
    }
}
@media (max-width: 768px) {
    .navigation {
        display: none;
    }
    .navigation ul {
        margin: 0;
        padding: 0;
    }
    .header {
        display: grid;
    }
    .header-right.header__icons {
        display: flex;
    }
    .header__icon {
        height: 4.4rem;
        width: 4.4rem;
        padding: 0;
    }
    .header-right .small-hide {
        display: flex !important;
    }
}



.tmenu_item {
    .tmenu_item_link {
        text-transform: uppercase;
        font-size: 1.2rem;
    }
}
.header--top-center > .header__search {
    display: none;
}
.header--top-center * > .header__search {
    display: inline-flex;
    align-items: end;
}
.header__icons {
    padding-right: 0;
}
@media screen and (min-width: 990px) {
    .header:not(.header--middle-left,.header--middle-center) .header__inline-menu {
        margin-top: 0;
    }
}
.header__menu-item {
    padding: 0;
    font-family: "Public Sans", Helvetica, sans-serif;
}
.header-logo .header__heading-link {
    padding: 0;
}
@media screen and (min-width: 990px) {
    .header__icons {
        grid-area: navigation;
    }
}

/* override Mega Menu styles */
.tmenu_item--root > .tmenu_submenu {
    box-shadow: none;
}

/* Custom width */
@media screen and (min-width: 769px) {
    .header:not(.drawer-menu).page-width, .banner {
        max-width: 1188px;
        margin-left: auto;
        margin-right: auto;
        padding-left: 16px;
        padding-right: 16px;
    }
    .tmenu_item--root>.tmenu_item_link:first-of-type {
        padding-left: 0 !important;
    }
}

/* Banner */
.banner__buttons a {
    padding: 12px 48px;
    font-size: 12px;
    letter-spacing: normal;
    font-weight: 700;
    border-radius: 4px;
    transition: 0.5s ease;
}
.banner__buttons a:hover {
    --border-offset: none;
    box-shadow: unset;
    color: #000;
    border: 1px solid #000;
    background-color: #fff;
}
.mobile {
    display: none;
}
.banner-desc,
.banner-desc-other {
    justify-content: flex-start;
    display: flex;
    flex-direction: column;
    background-color: #000;
    padding-top: 5px;
    padding-bottom: 5px;
    background-position: left top;
    background-size: cover;
    background-repeat: no-repeat;
    background-attachment: scroll;
    font-size: 13px;
}
.banner-desc {
    margin-bottom: 2px;
}
.banner-desc-other {
    margin-bottom: 33px;
}
._mobile {
    display: none;
} 
._desktop {
    display: block;
}  

@media screen and (max-width: 768px) {
    ._mobile {
        display: block;
    } 
    ._desktop {
        display: none;
    }  
}
@media screen and (min-width: 750px) {
    .banner {
        .banner__heading {
            color: #ffffff;
        }
    }
}

/* home introduction section */
.home-intro-section {
    display: flex;
    width: 100%;
}
.home-intro-section > div {
    justify-content: flex-start;
    display: flex;
    flex-direction: column;
    background-position: left top;
    background-size: cover;
    background-repeat: no-repeat;
    background-attachment: scroll;
    width: 50%;
    align-self: stretch;
}
.home-intro-section > div:first-child {
    margin-right: 78px;
}
.home-intro-section .intro-img {
    width: 539px;
}
.intro-text > div {
    padding: 0 !important;
}
.intro-text h2 {
    margin-bottom: 30px;
    font-size: 34px !important;
    line-height: 44px;
}
.intro-text .image-with-text__text p a {
    font-size: 18px;
    text-decoration: none;
    letter-spacing: normal;
    color: #000;
}
.intro-text .image-with-text__text p a:hover {
    text-decoration: underline;
}
.intro-text .image-with-text__text p:first-child a {
    display: flex;
    align-items: center;
    gap: 8px;
    text-decoration: none;
}
.intro-text .image-with-text__text p a:first-child:hover {
    text-decoration: underline;
}
.image-with-text__offer > p:last-child {
    box-sizing: border-box;
    padding-left: 58px;
}
.custom-arrow {
    display: block !important;
    position: relative;
    max-width: 48px;
    min-width: 24px;
    flex-grow: 1;
    height: 0;
    border-top: 1px solid #000;
}
.custom-arrow:before,
.custom-arrow:after {
    content: '';
    width: 6px;
    position: absolute;
    height: 0;
    border-top: 1px solid #000;
}
.custom-arrow:before {
    right: -1px;
    bottom: 2px;
    transform: rotate(45deg);
}
.custom-arrow:after {
    right: -1px;
    top: 1px;
    transform: rotate(-45deg);
}
@media (max-width: 768px) {
    .home-intro-section {
        flex-direction: column;
    }
    .home-intro-section > div {
        width: 100%;
    }
    .home-intro-section > div:first-child {
        margin-right: 0;
        margin-bottom: 16px;
    }
    .home-intro-section .intro-img {
        width: 100%;
        max-width: 100%;
    }
    .home-intro-section .intro-img > div {
        padding-bottom: 108% !important;
        border: none;
    }
    .home-intro-section .intro-img img {
        max-width: 500px !important;
        height: auto !important;
        right: 0;
        left: unset !important;
    }
    .intro-text h2 {
        font-size: 20px !important;
        line-height: 23px;
    }
    .intro-text .image-with-text__text p a { 
        font-size: 15px;
        line-height: 18px;
    }
    .image-with-text__offer > p:last-child {
        padding-left: 24px;
    }
}
@media (max-width: 579px) {
    .home-intro-section .intro-img > div {
        margin-left: 48px;
    }
}

/* Product card */
.product_tile_color_holder {
    display: flex;
    align-items: center; 
    gap: 5px;
}
.product-color {
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 3px;
}
.product_tile_color_holder > .product-color:first-child {
    padding: 1px;
    border: 1px solid #000;
}
input[type=radio][name$=_card_color]{
    width: 20px;
    height: 20px;
    /* border-radius: 50%; */
    border-radius: 3px;
    appearance: none;
    background-size: 9px 9px;
    background-image: repeating-linear-gradient(45deg, #000 0, #000 1px, #ebebeb 0, #ebebeb 50%);
    margin: 0;
    padding: 0;
}
.price__container {
    display: flex !important;
    align-items: center;
    justify-content: space-between;
    gap: 8px;
}
.price__container .custom-arrow {
    max-width: 100%;
}


/* Slider */
.home-featured-product-slider ul li {
    width: 274px !important;
}
.collection  .slider-buttons {
    z-index: 999;
    position: absolute;
    display: flex;
    align-items: center;
    justify-content: space-between !important;
    top: 50%;
    width: 85%;
    left: 11%;
    transform: translateY(-50%) !important;
}
.collection button.slider-button svg path {
    fill: #ffff !important;
}
.collection .slider-button {
    border: solid black;
    border-width: 0 1.5px 1.5px 0;
    display: inline-block;
    padding: 3px;
    width: 22px;
    height: 22px;
}
.slider-button--prev {
    transform: rotate(135deg);
    -webkit-transform: rotate(135deg);
}
.slider-button--next {
    transform: rotate(-45deg);
    -webkit-transform: rotate(-45deg);
}
.collection .slider-counter.caption {
    display: none !important;
}
.slide-image__wrapper .slide-image__caption {
    display: block;
    position: absolute;
    left: 0;
    bottom: 0;
    padding: 24px;
    color: #fff;
    font-weight: 700;
    font-size: 15px;
    max-width: 50%;
    letter-spacing: normal;
}
.slide-image__wrapper .slide-image__caption p {
    margin: 0;
}

.slick-prev:before, .slick-next:before, .slick-prev:after, .slick-next:after {
    color: #555 !important;
    font-size: 38px;
    opacity: 1;
}

/* .widget-product-carousel .slider__slide{
    width: 251px !important;
} */

.slide-image__wrapper .slide-image {
    display: block;
    position: relative;
    width: 100%;
    aspect-ratio: 1.27;
    background-size: 120%;
    background-repeat: no-repeat;
    background-position: bottom 60% left 90%;
    transition: opacity .25s ease-in-out;
}

.slick-slider .product-item {
    width: 274px !important;
}

#custom-featured-product-slider .slick-slider .slick-slide:first-child {
    width: 572px;
    border-radius: 3px;
}

#custom-featured-product-slider, #home-featured-product-slider{
  padding: 0px 0px;
}

.product-item:nth-child(2n+1) {
    margin-left: 0;
}

.product-item {
    font-size: 1.4rem;
    vertical-align: top;
}

.slick-slider .slick-track {
    min-width: 100%;
    display: flex;
    gap: 24px;
}

.product-items {
    margin: 0;
    padding: 0;
    font-size: 0;
}

.slick-slider .slick-prev:before {
    background: url('{{ 'chevron-left.svg' | asset_url }}') no-repeat;
}

.slick-slider .slick-next:before {
    background: url('{{ 'chevron-right.svg' | asset_url }}') no-repeat;
}
.slick-slider .slick-prev:before, .slick-slider .slick-next:before {
    display: block;
    content: '';
    width: 16px;
    height: 32px;
    background-size: contain;
}
.card__heading {
    font-size: 15px;
    letter-spacing: normal;
    line-height: 1.4;
    /* min-height: 42px; */
    max-height: 23px;
    overflow: hidden;
}
.card--standard>.card__content .card__information {
    padding: 0 !important;
    padding-top: 14px !important;
}
.price__container {
    font-size: 15px;
}

@media (max-width: 600px) {
    .collection  .slider-buttons {
        width: 88% !important;
        left: 2% !important;
    }
}
@media screen and (min-width: 750px) {
    .grid {
        &.product-grid {
            column-gap: 25px !important;
        }
    }

}
@media screen and (min-width: 750px) {
    .page-width.collection__title {
        padding: 0;
    }
}
@media screen and (min-width: 990px) {
    .slider--desktop.grid--4-col-desktop .grid__item {
        width: calc((94% - var(--desktop-margin-left-first-item)) / 4 - var(--grid-desktop-horizontal-spacing)* 3) !important;
    }
